<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<display-name>LogoutServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteStudentServlet</servlet-name>
		<display-name>DeleteStudentServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.DeleteStudentServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginValidationServlet</servlet-name>
		<display-name>LoginValidationServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.LoginValidationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateStudentServlet</servlet-name>
		<display-name>UpdateStudentServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.UpdateStudentServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SaveStudent</servlet-name>
		<display-name>SaveStudent</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.SaveStudent</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterUserServlet</servlet-name>
		<display-name>RegisterUserServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.RegisterUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditExamServlet</servlet-name>
		<display-name>EditExamServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.EditExamServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteExamServlet</servlet-name>
		<display-name>DeleteExamServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.DeleteExamServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SaveExamServlet</servlet-name>
		<display-name>SaveExamServlet</display-name>
		<description></description>
		<servlet-class>com.dxc.servlet.SaveExamServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteStudentServlet</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginValidationServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateStudentServlet</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SaveStudent</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterUserServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditExamServlet</servlet-name>
		<url-pattern>/editexam</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteExamServlet</servlet-name>
		<url-pattern>/deleteexam</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SaveExamServlet</servlet-name>
		<url-pattern>/saveexam</url-pattern>
	</servlet-mapping>
</web-app>
